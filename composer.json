{
    "name": "larsnieuwenhuizen/es-connector",
    "type": "library",
    "license": "MIT",
    "description": "Package to use objects reflecting Elasticsearch's object architecture and ways to configure and use indices",
    "keywords": [
        "elastic", "elasticsearch"
    ],
    "authors": [
        {
            "name": "Lars Nieuwenhuizen"
        }
    ],
    "autoload": {
        "psr-4": {
            "LarsNieuwenhuizen\\EsConnector\\": "src/"
        }
    },
    "require": {
        "php": "^7.4",
        "elasticsearch/elasticsearch": "^7.0",
        "neos/utility-arrays": ">=5.1",
        "psr/log": "^1.1",
        "symfony/console": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
